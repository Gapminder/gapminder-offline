{
  "name": "gapminder-offline",
  "version": "0.70.17",
  "description": "Gapminder offline tool",
  "main": "",
  "scripts": {
    "watch": "npm run watch:dev",
    "watch:dev": "webpack --watch --progress --profile --colors --display-error-details --display-cached",
    "build": "npm run build:dev",
    "build-win": "npm run build-win:dev",
    "vizabi": "cp ./node_modules/vizabi/build/dist/vizabi.js ./src/app",
    "vizabi-config": "cp ./node_modules/vizabi-config-systema_globalis/* ./src/app/vizabi-config",
    "vizabi-styles": "cp ./node_modules/vizabi/build/dist/vizabi.css ./src/app",
    "vizabi-cursors": "cp ./node_modules/vizabi/build/dist/assets/cursors/* ./src/app/assets/cursors",
    "vizabi-translation": "cp ./node_modules/vizabi/build/dist/assets/translation/* ./src/app/preview-data/translation && cp ./node_modules/vizabi/build/dist/assets/translation/* ./src/app/assets/translation",
    "build:dev": "npm run vizabi && npm run vizabi-config && npm run vizabi-styles && npm run vizabi-cursors && npm run vizabi-translation && webpack --progress --profile --colors --display-error-details --display-cached",
    "vizabi-win": "copy .\\node_modules\\vizabi\\build\\dist\\vizabi.js .\\src\\app",
    "vizabi-config-win": "copy .\\node_modules\\vizabi-config-systema_globalis\\* .\\src\\app\\vizabi-config",
    "vizabi-styles-win": "copy .\\node_modules\\vizabi\\build\\dist\\vizabi.css .\\src\\app",
    "vizabi-cursors-win": "copy .\\node_modules\\vizabi\\build\\dist\\assets\\cursors\\* .\\src\\app\\assets\\cursors",
    "vizabi-translation-win": "copy .\\node_modules\\vizabi\\build\\dist\\assets\\translation\\* .\\src\\app\\preview-data\\translation && copy .\\node_modules\\vizabi\\build\\dist\\assets\\translation\\* .\\src\\app\\assets\\translation",
    "build-win:dev": "npm run vizabi-win && npm run vizabi-config-win && npm run vizabi-styles-win && npm run vizabi-cursors-win && npm run vizabi-translation-win && webpack --progress --profile --colors --display-error-details --display-cached",
    "package": "node package.js",
    "package-win": "npm run package -- --platform=win32",
    "package-win32": "npm run package -- --platform=win32 --arch=ia32",
    "package-lin": "npm run package -- --platform=linux",
    "package-mac": "npm run package -- --platform=darwin && codesign --deep --force --verbose --sign 'Developer ID Application: Stiftelsen Gapminder (J7SD4Z498H)' ./release/Gapminder\\ Offline-darwin-x64/Gapminder\\ Offline.app",
    "package-all": "npm run package-win && npm run package-lin && npm run package-mac",
    "package-dmg": "node package-dmg.js",
    "mac": "rm -rf release && npm run build && npm run package-mac",
    "typings-install": "typings install",
    "electron": "NODE_ENV=development electron src/app",
    "webpack-test": "webpack --config webpack.test.js --progress --profile --colors --display-error-details --display-cached",
    "test": "karma start || true"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/VS-work/gapminder-offline.git"
  },
  "author": "Vyacheslav Chub <vyacheslav.chub@valor-software.com>",
  "license": "GPL-3.0",
  "homepage": "",
  "dependencies": {
    "@angular/common": "2.4.3",
    "@angular/compiler": "2.4.3",
    "@angular/core": "2.4.3",
    "@angular/forms": "2.4.3",
    "@angular/http": "2.4.3",
    "@angular/platform-browser": "2.4.3",
    "@angular/platform-browser-dynamic": "2.4.3",
    "@angular/platform-server": "2.4.3",
    "@angular/router": "3.4.3",
    "@types/core-js": "0.9.35",
    "@types/node": "7.0.0",
    "@types/zone.js": "0.0.27",
    "async": "2.1.4",
    "core-js": "2.4.1",
    "csv-parse": "1.1.9",
    "electron": "1.4.14",
    "lodash": "4.17.4",
    "minimist": "1.2.0",
    "ng2-bootstrap": "1.3.0",
    "ng2-vizabi": "0.19.3",
    "rxjs": "5.0.3",
    "vizabi": "0.19.1",
    "vizabi-config-systema_globalis": "git+https://github.com/vizabi/vizabi-config-systema_globalis.git",
    "vizabi-ddfcsv-reader": "0.7.2",
    "webpack-target-electron-renderer": "0.4.0",
    "zone.js": "0.7.2"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.41",
    "@types/webpack": "2.2.1",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "3.0.0-beta.18",
    "codelyzer": "2.0.0-beta.4",
    "electron-easy-updater": "0.2.9",
    "electron-packager": "8.5.0",
    "es6-promise-loader": "1.0.2",
    "es6-shim": "0.35.2",
    "es7-reflect-metadata": "1.6.0",
    "extract-text-webpack-plugin": "1.0.1",
    "imports-loader": "0.7.0",
    "istanbul-instrumenter-loader": "1.2.0",
    "jasmine-core": "2.5.2",
    "json-loader": "0.5.4",
    "karma": "1.4.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-electron": "^5.1.1",
    "karma-jasmine": "1.1.0",
    "karma-mocha-reporter": "2.2.1",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-remap-istanbul": "^0.6.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.1",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "2.1.14",
    "raw-loader": "0.5.1",
    "reflect-metadata": "0.1.9",
    "remap-istanbul": "0.8.4",
    "rimraf": "2.5.4",
    "source-map-loader": "0.1.6",
    "to-string-loader": "1.1.5",
    "ts-helpers": "1.1.2",
    "ts-loader": "1.3.3",
    "tslint": "^4.3.1",
    "tslint-loader": "3.3.0",
    "typescript": "2.1.5",
    "typings": "2.1.0",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2"
  },
  "optionalDependencies": {
    "appdmg": "0.4.5"
  },
  "engines": {
    "node": ">= 4.2.1 <= 6",
    "npm": ">= 3"
  }
}
