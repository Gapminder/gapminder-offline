{
  "name": "gapminder-offline",
  "version": "4.1.0",
  "description": "Gapminder Tools Offline",
  "author": "Gapminder <ola@gapminder.org>",
  "repository": {
    "type": "git",
    "url": "https://github.com/Gapminder/gapminder-offline.git"
  },
  "license": "GPL-3.0",
  "keywords": [
    "angular",
    "angular 6",
    "electron",
    "typescript",
    "sass"
  ],
  "main": "main.js",
  "googleAnalyticsId": "UA-67908993-4",
  "private": true,
  "scripts": {
    "postinstall": "npx electron-builder install-app-deps",
    "ng": "ng",
    "start": "npm-run-all -p ng:serve electron:serve",
    "build": "npm run electron:tsc && ng build",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve -o",
    "electron:tsc": "tsc --typeRoots [\\\"node_modules/@types\\\"] main.ts",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:tsc && electron . --serve",
    "electron:local": "npm run build:dev && electron .",
    "electron:linux": "npm run build:dev && npx electron-builder build --linux && ./scripts/additional-copy-linux",
    "electron:windows": "npm run build:dev && npx electron-builder build --windows",
    "electron:windows32": "npm run build:dev && npx electron-builder build --windows --ia32",
    "electron:mac": "npm run build:dev && npx electron-builder build --mac && ./scripts/additional-copy-mac",
    "rebuild": "electron-rebuild -f -w gapminder-offline",
    "package-dmg": "npm run rebuild && npm rebuild fs-xattr && npm rebuild macos-alias && node package-dmg.js",
    "start-webdriver": "node node_modules/electron-chromedriver/chromedriver &",
    "e2e-test": "protractor ./e2e/protractor.conf.js",
    "e2e:linux": "npm run start-webdriver && npm run e2e-test",
    "e2e:windows": "start npm run start-webdriver && npm run e2e-test",
    "update-webdriver": "webdriver-manager update",
    "test": "ng test",
    "e2e": "ng e2e",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -p angular",
    "github-release": "conventional-github-releaser -p angular",
    "version": "npm run changelog && git add CHANGELOG.md",
    "postversion": "git push origin development && git push --tags && npm run github-release"
  },
  "dependencies": {
    "async": "2.6.1",
    "async-waterfall": "0.1.5",
    "csv-parse": "4.3.1",
    "ddf-validation": "1.19.0",
    "electron-easy-updater": "0.7.1",
    "folder-hash": "2.1.2",
    "fs-extra": "7.0.1",
    "git-fetch-pack": "0.1.1",
    "git-transport-protocol": "0.1.0",
    "is-online": "8.0.0",
    "lodash": "4.17.11",
    "minimist": "1.2.0",
    "node-fetch": "2.3.0",
    "ps-node": "0.1.6",
    "semver": "5.6.0",
    "semver-sort": "0.0.4",
    "tslib": "1.9.3",
    "universal-analytics": "0.4.20",
    "urlon": "3.0.1",
    "vizabi-csv-reader": "0.4.2",
    "vizabi-ddfcsv-reader": "3.0.1",
    "vizabi-excel-reader": "0.4.1",
    "zip-dir": "1.0.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.12.2",
    "@angular/cli": "7.2.2",
    "@angular/common": "7.2.2",
    "@angular/compiler": "7.2.2",
    "@angular/compiler-cli": "7.2.2",
    "@angular/core": "7.2.2",
    "@angular/forms": "7.2.2",
    "@angular/http": "7.2.2",
    "@angular/language-service": "7.2.2",
    "@angular/platform-browser": "7.2.2",
    "@angular/platform-browser-dynamic": "7.2.2",
    "@angular/router": "7.2.2",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "@types/core-js": "2.5.0",
    "@types/jasmine": "3.3.7",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "10.12.19",
    "codelyzer": "4.5.0",
    "compare-version": "0.1.2",
    "conventional-changelog": "3.0.5",
    "conventional-changelog-cli": "2.0.11",
    "conventional-github-releaser": "3.1.2",
    "core-js": "2.6.3",
    "d3": "5.9.1",
    "electron": "4.0.2",
    "electron-builder": "20.38.5",
    "electron-chromedriver": "3.0.0",
    "electron-rebuild": "1.8.2",
    "electron-reload": "1.4.0",
    "jasmine-core": "3.3.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "3.1.4",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.4",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.0",
    "ngx-bootstrap": "3.1.4",
    "npm-run-all": "4.1.5",
    "npx": "10.2.0",
    "protractor": "5.4.2",
    "rxjs": "6.3.3",
    "rxjs-compat": "6.3.3",
    "ts-node": "8.0.1",
    "tslint": "5.12.1",
    "typescript": "3.2.4",
    "vizabi": "0.40.0",
    "vizabi-barrankchart": "2.4.2",
    "vizabi-bubblechart": "2.4.1",
    "vizabi-bubblemap": "2.3.1",
    "vizabi-config-systema_globalis": "2.9.1",
    "vizabi-linechart": "2.3.1",
    "vizabi-mountainchart": "2.4.2",
    "wait-on": "3.2.0",
    "webdriver-manager": "12.1.1",
    "zone.js": "0.8.29"
  },
  "optionalDependencies": {
    "appdmg": "0.5.2"
  }
}
