{
  "name": "gapminder-offline",
  "version": "2.2.2",
  "description": "Gapminder Tools Offline",
  "main": "",
  "scripts": {
    "watch": "npm run watch:dev",
    "watch:dev": "webpack --watch --progress --profile --colors --display-error-details --display-cached",
    "build-linux": "npm run build-linux:dev",
    "build-mac": "npm run build-mac:dev",
    "build-win": "npm run build-win:dev",
    "vizabi-linux": "./vizabi-copy-linux",
    "vizabi-mac": "./vizabi-copy-mac",
    "build-linux:dev": "npm run vizabi-linux && webpack --progress --profile --colors --display-error-details --display-cached",
    "build-mac:dev": "npm run vizabi-mac && webpack --progress --profile --colors --display-error-details --display-cached",
    "vizabi-win": ".\\vizabi-copy-win.bat",
    "build-win:dev": "npm run vizabi-win && webpack --progress --profile --colors --display-error-details --display-cached",
    "package": "node package.js",
    "package-win": "npm run package -- --platform=win32",
    "package-win32": "npm run package -- --platform=win32 --arch=ia32",
    "package-lin": "npm run package -- --platform=linux",
    "package-mac": "npm run package -- --platform=darwin && codesign --deep --force --verbose --sign 'Developer ID Application: Stiftelsen Gapminder (J7SD4Z498H)' ./release/Gapminder\\ Offline-darwin-x64/Gapminder\\ Offline.app",
    "package-all": "npm run package-win && npm run package-lin && npm run package-mac",
    "package-dmg": "node package-dmg.js",
    "mac": "rm -rf release && npm run build && npm run package-mac",
    "typings-install": "typings install",
    "electron": "NODE_ENV=development electron src/app",
    "webpack-test": "webpack --config webpack.test.js --progress --profile --colors --display-error-details --display-cached",
    "test": "karma start || true",
    "ncu-vizabi": "ncu -u vizabi vizabi-barrankchart vizabi-bubblechart vizabi-bubblemap vizabi-config-systema_globalis vizabi-ddfcsv-reader vizabi-linechart vizabi-mountainchart vizabi-popbyage && npm i && git add package.json && git commit -m 'chore(package): update vizabis'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/VS-work/gapminder-offline.git"
  },
  "author": "Vyacheslav Chub <vyacheslav.chub@valor-software.com>",
  "license": "GPL-3.0",
  "homepage": "",
  "dependencies": {
    "@angular/common": "4.4.6",
    "@angular/compiler": "4.4.6",
    "@angular/core": "4.4.6",
    "@angular/forms": "4.4.6",
    "@angular/http": "4.4.6",
    "@angular/platform-browser": "4.4.6",
    "@angular/platform-browser-dynamic": "4.4.6",
    "@angular/platform-server": "4.4.6",
    "@angular/router": "4.4.6",
    "@types/core-js": "0.9.43",
    "@types/node": "8.0.47",
    "@types/zone.js": "0.5.12",
    "async": "2.5.0",
    "async-waterfall": "0.1.5",
    "core-js": "2.4.1",
    "csv-parse": "1.2.0",
    "electron": "1.6.11",
    "lodash": "4.17.4",
    "minimist": "1.2.0",
    "ng2-bootstrap": "1.6.3",
    "ng2-vizabi": "2.0.0",
    "rxjs": "5.5.1",
    "vizabi": "0.26.6-4",
    "vizabi-barrankchart": "1.0.23",
    "vizabi-bubblechart": "1.0.23-0",
    "vizabi-bubblemap": "1.0.16",
    "vizabi-config-systema_globalis": "1.3.0",
    "vizabi-ddfcsv-reader": "0.14.0",
    "vizabi-linechart": "1.0.20-0",
    "vizabi-mountainchart": "1.0.20-0",
    "vizabi-popbyage": "1.2.5-0",
    "webpack-target-electron-renderer": "0.4.0",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
