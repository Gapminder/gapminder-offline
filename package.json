{
  "name": "gapminder-offline",
  "version": "0.17.1",
  "description": "Gapminder offline tool",
  "main": "",
  "scripts": {
    "watch": "npm run watch:dev",
    "watch:dev": "webpack --watch --progress --profile --colors --display-error-details --display-cached",
    "build": "npm run build:dev",
    "build:dev": "webpack --progress --profile --colors --display-error-details --display-cached",
    "package": "node package.js",
    "package-win": "npm run package -- --platform=win32",
    "package-lin": "npm run package -- --platform=linux",
    "package-mac": "npm run package -- --platform=darwin",
    "package-all": "npm run package-win && npm run package-lin && npm run package-mac",
    "typings-install": "typings install",
    "postinstall": "npm run typings-install",
    "electron": "NODE_ENV=development electron src/app",
    "webpack-test": "webpack --config webpack.test.js --progress --profile --colors --display-error-details --display-cached",
    "test": "karma start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/VS-work/gapminder-offline.git"
  },
  "author": "Vyacheslav Chub <vyacheslav.chub@valor-software.com>",
  "license": "GPL-3.0",
  "homepage": "",
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/platform-server": "2.0.0",
    "@angular/router": "3.0.0",
    "async": "2.0.1",
    "core-js": "2.4.1",
    "csv-parse": "1.1.7",
    "electron": "1.4.3",
    "format-json": "1.0.3",
    "lodash": "4.16.2",
    "ng2-bootstrap": "1.1.5",
    "ng2-vizabi": "0.17.1-3",
    "path": "0.12.7",
    "rxjs": "5.0.0-beta.12",
    "vizabi-ddfcsv-reader": "0.4.18",
    "webpack-target-electron-renderer": "0.4.0",
    "zone.js": "0.6.25"
  },
  "devDependencies": {
    "awesome-typescript-loader": "2.2.4",
    "codelyzer": "1.0.0-beta.0",
    "electron-packager": "8.0.0",
    "es6-promise-loader": "1.0.2",
    "es6-shim": "0.35.1",
    "es7-reflect-metadata": "1.6.0",
    "extract-text-webpack-plugin": "1.0.1",
    "imports-loader": "0.6.5",
    "istanbul-instrumenter-loader": "1.0.0",
    "jasmine-core": "2.5.2",
    "json-loader": "0.5.4",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.0.2",
    "karma-mocha-reporter": "2.2.0",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.8.0",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "2.1.12",
    "raw-loader": "0.5.1",
    "redux": "3.6.0",
    "reflect-metadata": "0.1.8",
    "remap-istanbul": "0.6.4",
    "rimraf": "2.5.4",
    "source-map-loader": "0.1.5",
    "to-string-loader": "1.1.5",
    "ts-helpers": "1.1.1",
